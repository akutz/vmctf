[Unit]
Description=containerd_.service
Documentation=https://containerd.io
After=bins.service
Requires=bins.service

[Install]
WantedBy=multi-user.target

[Service]
Restart=always
RestartSec=5
Delegate=yes
KillMode=process
OOMScoreAdjust=-999
LimitNOFILE=1048576
LimitNPROC=infinity
LimitCORE=infinity
WorkingDirectory=${working_directory}
ExecStartPre=/sbin/modprobe overlay
ExecStart=${cmd_file} -c ${config_file}
